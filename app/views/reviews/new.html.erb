<h1>Add a Review</h1>

<div class="container">
  <div class="row">
    <div class="col-md-6 mx-auto">
      <%= form_for [@restaurant, @review ] do |f| %>
       <%= f.label :content %>
       <%= f.text_field :content %>

       <%= f.label :rating %>
      <%= f.select :rating, [['1'],['2'],['3'],['4'],['5']] %>
      <%= f.submit %>
  <% end %>
